{
    "name": "renoki-co/laravel-explicit-array",
    "description": "Improved Laravel dot notation for explicit array keys.",
    "keywords": ["laravel", "php", "arr", "support", "array"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/laravel-explicit-array",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "^7.40|^8.40"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\ExplicitArray\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\ExplicitArray\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.23.2",
        "vimeo/psalm": "4.18.1"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\ExplicitArray\\ExplicitArrayServiceProvider"
            ]
        }
    }
}
